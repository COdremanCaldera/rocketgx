						<p><a href="shop.html" class="btn">Explorar</a></p>
					</div>
					<!-- End Column 1 -->

					<!-- Start Column 2 -->
					<div class="col-12 col-md-4 col-lg-3 mb-5 mb-md-0">
						<a class="product-item" href="cart.html">
							<%= image_tag "https://cdn-products.eneba.com/resized-products/s29Db6ZBVLneuD0t66qnYRamvGDP3p8chLz-3IomxcU_350x200_1x-0.jpeg", class:"img-fluid product-thumbnail" %>
							<h3 class="product-title">Grand Theft Auto V: Edición Online Premium Rockstar Games</h3>
							<strong class="product-price">$50.00</strong>

							<span class="icon-cross">
								<%=image_tag "cross.svg", class:"img-fluid"%>
							</span>
						</a>
					</div>
